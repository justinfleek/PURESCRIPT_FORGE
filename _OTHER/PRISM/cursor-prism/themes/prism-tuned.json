    "scrollbarSlider.hoverBackground": "#3a424f60",
    "scrollbarSlider.activeBackground": "#3a424f80"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#3a424f",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#54aeff"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#87c6ff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#76beff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c5d0dd"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c7d2de"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#7ec2ff"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#c5d0dd"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#54aeff"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#6dbaff"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#3a424f"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#54aeff"
      }
    }
  ]
}