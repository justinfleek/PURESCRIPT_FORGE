  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7ee787",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#58a6ff"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#8ac0ff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#79b7ff"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#bc8cff"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#bf91ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#81bcff"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#bc8cff"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#58a6ff"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#71b3ff"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#58a6ff"
      }
    }
  ]
}