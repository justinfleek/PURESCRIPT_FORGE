    "scrollbarSlider.activeBackground": "#88c0d080"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#616e88",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#a6bdd3"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#9ab3cd"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#bf616a"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c26871"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#a0b8d0"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#bf616a"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#93afca"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#88c0d0"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#81a1c1"
      }
    }
  ]
}