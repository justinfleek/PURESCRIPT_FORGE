  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7a8a9c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4080c4"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#366ca6"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#3973b0"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#384452"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#323d49"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#3c79ba"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#384452"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#4080c4"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#3973b0"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#7a8a9c"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#4080c4"
      }
    }
  ]
}