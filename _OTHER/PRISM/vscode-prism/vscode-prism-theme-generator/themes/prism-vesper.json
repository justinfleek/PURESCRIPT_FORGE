  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#C39B83",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#676266"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#575356"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#5c585b"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#272727"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#232323"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#615d60"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#272727"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#676266"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#5c585b"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#C39B83"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#676266"
      }
    }
  ]
}