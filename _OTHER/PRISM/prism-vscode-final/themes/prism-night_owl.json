    "scrollbarSlider.hoverBackground": "#82aaff60",
    "scrollbarSlider.activeBackground": "#82aaff80"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#82aaff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#d7b2f0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#d2a7ee"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f79173"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#d5adef"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#f78c6c"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c792ea"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#cfa2ed"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#c792ea"
      }
    }
  ]
}