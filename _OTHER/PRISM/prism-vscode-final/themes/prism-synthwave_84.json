  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#614d73",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#36f9f6"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#72faf8"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#5efaf7"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ff8b39"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ff9042"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#68faf8"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#ff8b39"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#36f9f6"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#54f9f7"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#ff7edb"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#36f9f6"
      }
    }
  ]
}