{
  "name": "Prism Tessier",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#1a2028",
    "editorLineNumber.foreground": "#6b7a8a",
    "editorLineNumber.activeForeground": "#1a2028",
    "editorCursor.foreground": "#0758c9",
    "editor.selectionBackground": "#0758c930",
    "editor.selectionHighlightBackground": "#0758c920",
    "editor.wordHighlightBackground": "#0758c925",
    "editor.findMatchBackground": "#0758c940",
    "editor.findMatchHighlightBackground": "#0758c925",
    "editor.lineHighlightBackground": "#f0f3f6",
    "editorWhitespace.foreground": "#6b7a8a40",
    "editorIndentGuide.background": "#f0f3f6",
    "editorIndentGuide.activeBackground": "#6b7a8a",
    "editorBracketMatch.background": "#0758c925",
    "editorBracketMatch.border": "#0758c9",
    "sideBar.background": "#f0f3f6",
    "sideBar.foreground": "#1a2028",
    "sideBar.border": "#f0f3f6",
    "sideBarTitle.foreground": "#1a2028",
    "activityBar.background": "#ffffff",
    "activityBar.foreground": "#1a2028",
    "activityBar.border": "#f0f3f6",
    "activityBar.activeBorder": "#0758c9",
    "activityBarBadge.background": "#0758c9",
    "activityBarBadge.foreground": "#ffffff",
    "titleBar.activeBackground": "#ffffff",
    "titleBar.activeForeground": "#1a2028",
    "titleBar.inactiveBackground": "#f0f3f6",
    "titleBar.inactiveForeground": "#6b7a8a",
    "statusBar.background": "#ffffff",
    "statusBar.foreground": "#6b7a8a",
    "statusBar.border": "#f0f3f6",
    "statusBar.debuggingBackground": "#0758c9",
    "statusBar.debuggingForeground": "#ffffff",
    "tab.activeBackground": "#ffffff",
    "tab.activeForeground": "#1a2028",
    "tab.activeBorder": "#0758c9",
    "tab.inactiveBackground": "#f0f3f6",
    "tab.inactiveForeground": "#6b7a8a",
    "tab.border": "#f0f3f6",
    "panel.background": "#ffffff",
    "panel.border": "#f0f3f6",
    "panelTitle.activeBorder": "#0758c9",
    "panelTitle.activeForeground": "#1a2028",
    "panelTitle.inactiveForeground": "#6b7a8a",
    "terminal.background": "#ffffff",
    "terminal.foreground": "#1a2028",
    "terminal.ansiBlack": "#1a2028",
    "terminal.ansiRed": "#054aaa",
    "terminal.ansiGreen": "#1f68ce",
    "terminal.ansiYellow": "#6b7a8a",
    "terminal.ansiBlue": "#0758c9",
    "terminal.ansiMagenta": "#054296",
    "terminal.ansiCyan": "#2c71d1",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#6b7a8a",
    "terminal.ansiBrightRed": "#0758c9",
    "terminal.ansiBrightGreen": "#2c71d1",
    "terminal.ansiBrightYellow": "#798795",
    "terminal.ansiBrightBlue": "#1f68ce",
    "terminal.ansiBrightMagenta": "#064fb4",
    "terminal.ansiBrightCyan": "#4581d6",
    "terminal.ansiBrightWhite": "#ffffff",
    "gitDecoration.modifiedResourceForeground": "#0758c9",
    "gitDecoration.deletedResourceForeground": "#0546a0",
    "gitDecoration.untrackedResourceForeground": "#1f68ce",
    "button.background": "#0758c9",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#064fb4",
    "input.background": "#ffffff",
    "input.foreground": "#1a2028",
    "input.border": "#f0f3f6",
    "input.placeholderForeground": "#6b7a8a",
    "list.activeSelectionBackground": "#0758c925",
    "list.activeSelectionForeground": "#1a2028",
    "list.hoverBackground": "#f0f3f6",
    "focusBorder": "#0758c9",
    "scrollbarSlider.background": "#6b7a8a40",
    "scrollbarSlider.hoverBackground": "#6b7a8a60",
    "scrollbarSlider.activeBackground": "#6b7a8a80"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6b7a8a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#0758c9"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#054aaa"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#064fb4"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#1a2028"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#171c24"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#0653be"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#1a2028"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#0758c9"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#064fb4"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#6b7a8a"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#0758c9"
      }
    }
  ]
}