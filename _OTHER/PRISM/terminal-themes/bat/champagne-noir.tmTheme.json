{
  "name": "Prism champagne-noir",
  "variables": {
    "bg": "#0a0a08",
    "fg": "#f0ece0"
  },
  "globals": {
    "background": "#0a0a08",
    "foreground": "#f0ece0",
    "caret": "#f7e7ce",
    "selection": "#44475a",
    "line_highlight": "#11110f"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#3a3a35",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a060e0"
    },
    {
      "scope": "string",
      "foreground": "#60c080"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#6090e0"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#e89060"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#f7e7ce"
    },
    {
      "scope": "variable",
      "foreground": "#f0ece0"
    }
  ]
}