{
  "name": "Prism dracula-pro",
  "variables": {
    "bg": "#0d0d14",
    "fg": "#f8f8f2"
  },
  "globals": {
    "background": "#0d0d14",
    "foreground": "#f8f8f2",
    "caret": "#bd93f9",
    "selection": "#44475a",
    "line_highlight": "#14141b"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#6272a4",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ff79c6"
    },
    {
      "scope": "string",
      "foreground": "#f1fa8c"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#50fa7b"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#bd93f9"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#8be9fd"
    },
    {
      "scope": "variable",
      "foreground": "#f8f8f2"
    }
  ]
}