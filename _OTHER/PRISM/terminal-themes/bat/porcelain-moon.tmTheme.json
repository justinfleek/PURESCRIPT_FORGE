{
  "name": "Prism porcelain-moon",
  "variables": {
    "bg": "#e8eaee",
    "fg": "#1a1c20"
  },
  "globals": {
    "background": "#e8eaee",
    "foreground": "#1a1c20",
    "caret": "#ca8a04",
    "selection": "#44475a",
    "line_highlight": "#e8eaee"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#a0a4aa",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#7c3aed"
    },
    {
      "scope": "string",
      "foreground": "#16a34a"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#2563eb"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ea580c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#ca8a04"
    },
    {
      "scope": "variable",
      "foreground": "#1a1c20"
    }
  ]
}