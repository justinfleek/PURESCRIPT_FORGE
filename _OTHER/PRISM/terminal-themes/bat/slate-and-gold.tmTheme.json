{
  "name": "Prism slate-and-gold",
  "variables": {
    "bg": "#12141a",
    "fg": "#d8dce8"
  },
  "globals": {
    "background": "#12141a",
    "foreground": "#d8dce8",
    "caret": "#ffd700",
    "selection": "#44475a",
    "line_highlight": "#191b20"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#505868",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a78bfa"
    },
    {
      "scope": "string",
      "foreground": "#4ade80"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ffd700"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#ffd700"
    },
    {
      "scope": "variable",
      "foreground": "#d8dce8"
    }
  ]
}