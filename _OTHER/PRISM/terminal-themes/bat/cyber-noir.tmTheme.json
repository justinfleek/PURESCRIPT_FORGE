{
  "name": "Prism cyber-noir",
  "variables": {
    "bg": "#0a0a0f",
    "fg": "#a0a8c0"
  },
  "globals": {
    "background": "#0a0a0f",
    "foreground": "#a0a8c0",
    "caret": "#00fff0",
    "selection": "#44475a",
    "line_highlight": "#111116"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#404858",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ff0080"
    },
    {
      "scope": "string",
      "foreground": "#00ff88"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#00fff0"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ff8800"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#00fff0"
    },
    {
      "scope": "variable",
      "foreground": "#a0a8c0"
    }
  ]
}