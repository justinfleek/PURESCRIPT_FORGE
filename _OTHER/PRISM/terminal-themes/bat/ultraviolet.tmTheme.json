{
  "name": "Prism ultraviolet",
  "variables": {
    "bg": "#080410",
    "fg": "#e0d0ff"
  },
  "globals": {
    "background": "#080410",
    "foreground": "#e0d0ff",
    "caret": "#bf00ff",
    "selection": "#44475a",
    "line_highlight": "#0f0b17"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#704090",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#bf00ff"
    },
    {
      "scope": "string",
      "foreground": "#00ff88"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#00aaff"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ff8800"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#bf00ff"
    },
    {
      "scope": "variable",
      "foreground": "#e0d0ff"
    }
  ]
}