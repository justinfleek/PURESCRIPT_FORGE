{
  "name": "Prism fleek",
  "variables": {
    "bg": "#0a0a0a",
    "fg": "#f0ece0"
  },
  "globals": {
    "background": "#0a0a0a",
    "foreground": "#f0ece0",
    "caret": "#3cb878",
    "selection": "#44475a",
    "line_highlight": "#111111"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#4a4540",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ff6b35"
    },
    {
      "scope": "string",
      "foreground": "#3cb878"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#00a0e4"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ffd93d"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#3cb878"
    },
    {
      "scope": "variable",
      "foreground": "#f0ece0"
    }
  ]
}