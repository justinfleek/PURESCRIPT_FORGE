{
  "name": "Prism lavender-dusk",
  "variables": {
    "bg": "#10081a",
    "fg": "#e0d0f0"
  },
  "globals": {
    "background": "#10081a",
    "foreground": "#e0d0f0",
    "caret": "#b388ff",
    "selection": "#44475a",
    "line_highlight": "#170f20"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#604878",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#b388ff"
    },
    {
      "scope": "string",
      "foreground": "#4ade80"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fb923c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#b388ff"
    },
    {
      "scope": "variable",
      "foreground": "#e0d0f0"
    }
  ]
}