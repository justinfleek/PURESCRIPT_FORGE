{
  "name": "Prism holographic",
  "variables": {
    "bg": "#0a0810",
    "fg": "#e8e0f8"
  },
  "globals": {
    "background": "#0a0810",
    "foreground": "#e8e0f8",
    "caret": "#00ffff",
    "selection": "#44475a",
    "line_highlight": "#110f17"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#505080",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ff0080"
    },
    {
      "scope": "string",
      "foreground": "#00ff80"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#0080ff"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ffcc00"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#00ffff"
    },
    {
      "scope": "variable",
      "foreground": "#e8e0f8"
    }
  ]
}