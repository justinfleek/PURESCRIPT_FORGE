{
  "name": "Prism rose-pine",
  "variables": {
    "bg": "#191724",
    "fg": "#e0def4"
  },
  "globals": {
    "background": "#191724",
    "foreground": "#e0def4",
    "caret": "#c4a7e7",
    "selection": "#44475a",
    "line_highlight": "#1f1d2a"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#6e6a86",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#31748f"
    },
    {
      "scope": "string",
      "foreground": "#f6c177"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#ebbcba"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#c4a7e7"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#9ccfd8"
    },
    {
      "scope": "variable",
      "foreground": "#e0def4"
    }
  ]
}