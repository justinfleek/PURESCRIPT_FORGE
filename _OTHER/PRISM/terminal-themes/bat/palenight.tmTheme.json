{
  "name": "Prism palenight",
  "variables": {
    "bg": "#1b1e2b",
    "fg": "#a6accd"
  },
  "globals": {
    "background": "#1b1e2b",
    "foreground": "#a6accd",
    "caret": "#c792ea",
    "selection": "#44475a",
    "line_highlight": "#212431"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#676e95",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#c792ea"
    },
    {
      "scope": "string",
      "foreground": "#c3e88d"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#82aaff"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#f78c6c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#ffcb6b"
    },
    {
      "scope": "variable",
      "foreground": "#a6accd"
    }
  ]
}