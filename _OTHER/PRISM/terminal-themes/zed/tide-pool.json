{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism tide-pool",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism tide-pool",
      "appearance": "dark",
      "style": {
        "background": "#06090c",
        "editor.background": "#06090c",
        "editor.foreground": "#e0f0f8",
        "editor.gutter.background": "#06090c",
        "editor.line_highlight.background": "#0d1013",
        "editor.active_line_number": "#40e0d0",
        "editor.wrap_guide": "#485560",
        "terminal.background": "#06090c",
        "terminal.foreground": "#e0f0f8",
        "terminal.ansi.black": "#06090c",
        "terminal.ansi.red": "#f87171",
        "terminal.ansi.green": "#00ff88",
        "terminal.ansi.yellow": "#40e0d0",
        "terminal.ansi.blue": "#00d4ff",
        "terminal.ansi.magenta": "#a78bfa",
        "terminal.ansi.cyan": "#00d4ff",
        "terminal.ansi.white": "#e0f0f8",
        "syntax": {
          "comment": {
            "color": "#283540",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#a78bfa",
            "font_weight": 600
          },
          "string": {
            "color": "#00ff88"
          },
          "function": {
            "color": "#40e0d0"
          },
          "number": {
            "color": "#fb923c"
          },
          "type": {
            "color": "#40e0d0"
          },
          "variable": {
            "color": "#e0f0f8"
          },
          "constant": {
            "color": "#fb923c"
          },
          "attribute": {
            "color": "#40e0d0"
          },
          "property": {
            "color": "#e0f0f8"
          },
          "punctuation": {
            "color": "#e0f0f8"
          },
          "operator": {
            "color": "#a78bfa"
          }
        }
      }
    }
  ]
}