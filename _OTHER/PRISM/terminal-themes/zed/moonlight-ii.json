{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism moonlight-ii",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism moonlight-ii",
      "appearance": "dark",
      "style": {
        "background": "#1b1d2c",
        "editor.background": "#1b1d2c",
        "editor.foreground": "#c8d3f5",
        "editor.gutter.background": "#1b1d2c",
        "editor.line_highlight.background": "#212332",
        "editor.active_line_number": "#82aaff",
        "editor.wrap_guide": "#636da6",
        "terminal.background": "#1b1d2c",
        "terminal.foreground": "#c8d3f5",
        "terminal.ansi.black": "#1b1d2c",
        "terminal.ansi.red": "#ff757f",
        "terminal.ansi.green": "#c3e88d",
        "terminal.ansi.yellow": "#ffc777",
        "terminal.ansi.blue": "#82aaff",
        "terminal.ansi.magenta": "#c099ff",
        "terminal.ansi.cyan": "#86e1fc",
        "terminal.ansi.white": "#c8d3f5",
        "syntax": {
          "comment": {
            "color": "#636da6",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#c099ff",
            "font_weight": 600
          },
          "string": {
            "color": "#c3e88d"
          },
          "function": {
            "color": "#82aaff"
          },
          "number": {
            "color": "#ff966c"
          },
          "type": {
            "color": "#ffc777"
          },
          "variable": {
            "color": "#c8d3f5"
          },
          "constant": {
            "color": "#ff966c"
          },
          "attribute": {
            "color": "#ffc777"
          },
          "property": {
            "color": "#c8d3f5"
          },
          "punctuation": {
            "color": "#c8d3f5"
          },
          "operator": {
            "color": "#c099ff"
          }
        }
      }
    }
  ]
}