{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism vaporwave-sunset",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism vaporwave-sunset",
      "appearance": "dark",
      "style": {
        "background": "#100818",
        "editor.background": "#100818",
        "editor.foreground": "#f0e0ff",
        "editor.gutter.background": "#100818",
        "editor.line_highlight.background": "#170f1e",
        "editor.active_line_number": "#00ffff",
        "editor.wrap_guide": "#8060a0",
        "terminal.background": "#100818",
        "terminal.foreground": "#f0e0ff",
        "terminal.ansi.black": "#100818",
        "terminal.ansi.red": "#ff6b9d",
        "terminal.ansi.green": "#00ffaa",
        "terminal.ansi.yellow": "#ffcc00",
        "terminal.ansi.blue": "#00ffff",
        "terminal.ansi.magenta": "#c44dff",
        "terminal.ansi.cyan": "#00ffff",
        "terminal.ansi.white": "#f0e0ff",
        "syntax": {
          "comment": {
            "color": "#705090",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ff6b9d",
            "font_weight": 600
          },
          "string": {
            "color": "#00ffaa"
          },
          "function": {
            "color": "#00ffff"
          },
          "number": {
            "color": "#ffcc00"
          },
          "type": {
            "color": "#c44dff"
          },
          "variable": {
            "color": "#f0e0ff"
          },
          "constant": {
            "color": "#ffcc00"
          },
          "attribute": {
            "color": "#c44dff"
          },
          "property": {
            "color": "#f0e0ff"
          },
          "punctuation": {
            "color": "#f0e0ff"
          },
          "operator": {
            "color": "#ff6b9d"
          }
        }
      }
    }
  ]
}