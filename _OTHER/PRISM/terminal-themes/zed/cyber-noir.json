{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism cyber-noir",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism cyber-noir",
      "appearance": "dark",
      "style": {
        "background": "#0a0a0f",
        "editor.background": "#0a0a0f",
        "editor.foreground": "#a0a8c0",
        "editor.gutter.background": "#0a0a0f",
        "editor.line_highlight.background": "#111116",
        "editor.active_line_number": "#00fff0",
        "editor.wrap_guide": "#505868",
        "terminal.background": "#0a0a0f",
        "terminal.foreground": "#a0a8c0",
        "terminal.ansi.black": "#0a0a0f",
        "terminal.ansi.red": "#ff0055",
        "terminal.ansi.green": "#00ff88",
        "terminal.ansi.yellow": "#ff8800",
        "terminal.ansi.blue": "#0088ff",
        "terminal.ansi.magenta": "#ff0080",
        "terminal.ansi.cyan": "#00fff0",
        "terminal.ansi.white": "#a0a8c0",
        "syntax": {
          "comment": {
            "color": "#404858",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ff0080",
            "font_weight": 600
          },
          "string": {
            "color": "#00ff88"
          },
          "function": {
            "color": "#00fff0"
          },
          "number": {
            "color": "#ff8800"
          },
          "type": {
            "color": "#00fff0"
          },
          "variable": {
            "color": "#a0a8c0"
          },
          "constant": {
            "color": "#ff8800"
          },
          "attribute": {
            "color": "#00fff0"
          },
          "property": {
            "color": "#a0a8c0"
          },
          "punctuation": {
            "color": "#a0a8c0"
          },
          "operator": {
            "color": "#ff0080"
          }
        }
      }
    }
  ]
}