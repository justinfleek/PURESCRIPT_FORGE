{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism neon-nexus",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism neon-nexus",
      "appearance": "dark",
      "style": {
        "background": "#040804",
        "editor.background": "#040804",
        "editor.foreground": "#d0ffd0",
        "editor.gutter.background": "#040804",
        "editor.line_highlight.background": "#0b0f0b",
        "editor.active_line_number": "#00ff88",
        "editor.wrap_guide": "#308830",
        "terminal.background": "#040804",
        "terminal.foreground": "#d0ffd0",
        "terminal.ansi.black": "#040804",
        "terminal.ansi.red": "#ff4444",
        "terminal.ansi.green": "#00ff88",
        "terminal.ansi.yellow": "#ffaa00",
        "terminal.ansi.blue": "#00aaff",
        "terminal.ansi.magenta": "#ff00ff",
        "terminal.ansi.cyan": "#00ffff",
        "terminal.ansi.white": "#d0ffd0",
        "syntax": {
          "comment": {
            "color": "#306030",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#00ff88",
            "font_weight": 600
          },
          "string": {
            "color": "#00ff88"
          },
          "function": {
            "color": "#00aaff"
          },
          "number": {
            "color": "#ffaa00"
          },
          "type": {
            "color": "#00ff88"
          },
          "variable": {
            "color": "#d0ffd0"
          },
          "constant": {
            "color": "#ffaa00"
          },
          "attribute": {
            "color": "#00ff88"
          },
          "property": {
            "color": "#d0ffd0"
          },
          "punctuation": {
            "color": "#d0ffd0"
          },
          "operator": {
            "color": "#00ff88"
          }
        }
      }
    }
  ]
}