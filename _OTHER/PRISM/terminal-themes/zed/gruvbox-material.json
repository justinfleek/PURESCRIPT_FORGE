{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism gruvbox-material",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism gruvbox-material",
      "appearance": "dark",
      "style": {
        "background": "#1d2021",
        "editor.background": "#1d2021",
        "editor.foreground": "#d4be98",
        "editor.gutter.background": "#1d2021",
        "editor.line_highlight.background": "#232627",
        "editor.active_line_number": "#d8a657",
        "editor.wrap_guide": "#7c6f64",
        "terminal.background": "#1d2021",
        "terminal.foreground": "#d4be98",
        "terminal.ansi.black": "#1d2021",
        "terminal.ansi.red": "#ea6962",
        "terminal.ansi.green": "#a9b665",
        "terminal.ansi.yellow": "#d8a657",
        "terminal.ansi.blue": "#7daea3",
        "terminal.ansi.magenta": "#d3869b",
        "terminal.ansi.cyan": "#89b482",
        "terminal.ansi.white": "#d4be98",
        "syntax": {
          "comment": {
            "color": "#7c6f64",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ea6962",
            "font_weight": 600
          },
          "string": {
            "color": "#a9b665"
          },
          "function": {
            "color": "#a9b665"
          },
          "number": {
            "color": "#d3869b"
          },
          "type": {
            "color": "#d8a657"
          },
          "variable": {
            "color": "#d4be98"
          },
          "constant": {
            "color": "#d3869b"
          },
          "attribute": {
            "color": "#d8a657"
          },
          "property": {
            "color": "#d4be98"
          },
          "punctuation": {
            "color": "#d4be98"
          },
          "operator": {
            "color": "#ea6962"
          }
        }
      }
    }
  ]
}