{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism night-owl",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism night-owl",
      "appearance": "dark",
      "style": {
        "background": "#011627",
        "editor.background": "#011627",
        "editor.foreground": "#d6deeb",
        "editor.gutter.background": "#011627",
        "editor.line_highlight.background": "#081c2d",
        "editor.active_line_number": "#7fdbca",
        "editor.wrap_guide": "#637777",
        "terminal.background": "#011627",
        "terminal.foreground": "#d6deeb",
        "terminal.ansi.black": "#011627",
        "terminal.ansi.red": "#ef5350",
        "terminal.ansi.green": "#addb67",
        "terminal.ansi.yellow": "#ffcb8b",
        "terminal.ansi.blue": "#82aaff",
        "terminal.ansi.magenta": "#c792ea",
        "terminal.ansi.cyan": "#7fdbca",
        "terminal.ansi.white": "#d6deeb",
        "syntax": {
          "comment": {
            "color": "#637777",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#c792ea",
            "font_weight": 600
          },
          "string": {
            "color": "#ecc48d"
          },
          "function": {
            "color": "#82aaff"
          },
          "number": {
            "color": "#f78c6c"
          },
          "type": {
            "color": "#ffcb8b"
          },
          "variable": {
            "color": "#d6deeb"
          },
          "constant": {
            "color": "#f78c6c"
          },
          "attribute": {
            "color": "#ffcb8b"
          },
          "property": {
            "color": "#d6deeb"
          },
          "punctuation": {
            "color": "#d6deeb"
          },
          "operator": {
            "color": "#c792ea"
          }
        }
      }
    }
  ]
}