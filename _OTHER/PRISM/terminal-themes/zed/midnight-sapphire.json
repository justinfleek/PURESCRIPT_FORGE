{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism midnight-sapphire",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism midnight-sapphire",
      "appearance": "dark",
      "style": {
        "background": "#050810",
        "editor.background": "#050810",
        "editor.foreground": "#e0e8f8",
        "editor.gutter.background": "#050810",
        "editor.line_highlight.background": "#0c0f17",
        "editor.active_line_number": "#c0c0d0",
        "editor.wrap_guide": "#4a5568",
        "terminal.background": "#050810",
        "terminal.foreground": "#e0e8f8",
        "terminal.ansi.black": "#050810",
        "terminal.ansi.red": "#f87171",
        "terminal.ansi.green": "#6ee7b7",
        "terminal.ansi.yellow": "#c0c0d0",
        "terminal.ansi.blue": "#60a5fa",
        "terminal.ansi.magenta": "#a78bfa",
        "terminal.ansi.cyan": "#67e8f9",
        "terminal.ansi.white": "#e0e8f8",
        "syntax": {
          "comment": {
            "color": "#2a3548",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#a78bfa",
            "font_weight": 600
          },
          "string": {
            "color": "#6ee7b7"
          },
          "function": {
            "color": "#60a5fa"
          },
          "number": {
            "color": "#fb923c"
          },
          "type": {
            "color": "#c0c0d0"
          },
          "variable": {
            "color": "#e0e8f8"
          },
          "constant": {
            "color": "#fb923c"
          },
          "attribute": {
            "color": "#c0c0d0"
          },
          "property": {
            "color": "#e0e8f8"
          },
          "punctuation": {
            "color": "#e0e8f8"
          },
          "operator": {
            "color": "#a78bfa"
          }
        }
      }
    }
  ]
}