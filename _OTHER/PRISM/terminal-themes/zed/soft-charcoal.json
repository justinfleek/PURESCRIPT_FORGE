{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism soft-charcoal",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism soft-charcoal",
      "appearance": "dark",
      "style": {
        "background": "#2a2a32",
        "editor.background": "#2a2a32",
        "editor.foreground": "#e8e8f0",
        "editor.gutter.background": "#2a2a32",
        "editor.line_highlight.background": "#303038",
        "editor.active_line_number": "#fbbf24",
        "editor.wrap_guide": "#70707a",
        "terminal.background": "#2a2a32",
        "terminal.foreground": "#e8e8f0",
        "terminal.ansi.black": "#2a2a32",
        "terminal.ansi.red": "#f87171",
        "terminal.ansi.green": "#4ade80",
        "terminal.ansi.yellow": "#fbbf24",
        "terminal.ansi.blue": "#60a5fa",
        "terminal.ansi.magenta": "#a78bfa",
        "terminal.ansi.cyan": "#22d3ee",
        "terminal.ansi.white": "#e8e8f0",
        "syntax": {
          "comment": {
            "color": "#50505a",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#a78bfa",
            "font_weight": 600
          },
          "string": {
            "color": "#4ade80"
          },
          "function": {
            "color": "#60a5fa"
          },
          "number": {
            "color": "#fb923c"
          },
          "type": {
            "color": "#fbbf24"
          },
          "variable": {
            "color": "#e8e8f0"
          },
          "constant": {
            "color": "#fb923c"
          },
          "attribute": {
            "color": "#fbbf24"
          },
          "property": {
            "color": "#e8e8f0"
          },
          "punctuation": {
            "color": "#e8e8f0"
          },
          "operator": {
            "color": "#a78bfa"
          }
        }
      }
    }
  ]
}