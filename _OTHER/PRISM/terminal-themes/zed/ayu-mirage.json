{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism ayu-mirage",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism ayu-mirage",
      "appearance": "dark",
      "style": {
        "background": "#1f2430",
        "editor.background": "#1f2430",
        "editor.foreground": "#cccac2",
        "editor.gutter.background": "#1f2430",
        "editor.line_highlight.background": "#252a36",
        "editor.active_line_number": "#ffcc66",
        "editor.wrap_guide": "#5c6773",
        "terminal.background": "#1f2430",
        "terminal.foreground": "#cccac2",
        "terminal.ansi.black": "#1f2430",
        "terminal.ansi.red": "#f28779",
        "terminal.ansi.green": "#87d96c",
        "terminal.ansi.yellow": "#ffcc66",
        "terminal.ansi.blue": "#5ccfe6",
        "terminal.ansi.magenta": "#d4bfff",
        "terminal.ansi.cyan": "#95e6cb",
        "terminal.ansi.white": "#cccac2",
        "syntax": {
          "comment": {
            "color": "#5c6773",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ffa759",
            "font_weight": 600
          },
          "string": {
            "color": "#d5ff80"
          },
          "function": {
            "color": "#ffd580"
          },
          "number": {
            "color": "#ffcc66"
          },
          "type": {
            "color": "#73d0ff"
          },
          "variable": {
            "color": "#cccac2"
          },
          "constant": {
            "color": "#ffcc66"
          },
          "attribute": {
            "color": "#73d0ff"
          },
          "property": {
            "color": "#cccac2"
          },
          "punctuation": {
            "color": "#cccac2"
          },
          "operator": {
            "color": "#ffa759"
          }
        }
      }
    }
  ]
}