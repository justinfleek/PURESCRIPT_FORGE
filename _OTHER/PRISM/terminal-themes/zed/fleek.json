{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism fleek",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism fleek",
      "appearance": "dark",
      "style": {
        "background": "#0a0a0a",
        "editor.background": "#0a0a0a",
        "editor.foreground": "#f0ece0",
        "editor.gutter.background": "#0a0a0a",
        "editor.line_highlight.background": "#111111",
        "editor.active_line_number": "#3cb878",
        "editor.wrap_guide": "#6a6560",
        "terminal.background": "#0a0a0a",
        "terminal.foreground": "#f0ece0",
        "terminal.ansi.black": "#0a0a0a",
        "terminal.ansi.red": "#ff6b35",
        "terminal.ansi.green": "#3cb878",
        "terminal.ansi.yellow": "#ffd93d",
        "terminal.ansi.blue": "#00a0e4",
        "terminal.ansi.magenta": "#b8a078",
        "terminal.ansi.cyan": "#00a0e4",
        "terminal.ansi.white": "#f0ece0",
        "syntax": {
          "comment": {
            "color": "#4a4540",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ff6b35",
            "font_weight": 600
          },
          "string": {
            "color": "#3cb878"
          },
          "function": {
            "color": "#00a0e4"
          },
          "number": {
            "color": "#ffd93d"
          },
          "type": {
            "color": "#3cb878"
          },
          "variable": {
            "color": "#f0ece0"
          },
          "constant": {
            "color": "#ffd93d"
          },
          "attribute": {
            "color": "#3cb878"
          },
          "property": {
            "color": "#f0ece0"
          },
          "punctuation": {
            "color": "#f0ece0"
          },
          "operator": {
            "color": "#ff6b35"
          }
        }
      }
    }
  ]
}