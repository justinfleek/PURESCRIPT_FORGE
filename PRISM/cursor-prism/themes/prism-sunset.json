    "scrollbarSlider.hoverBackground": "#9E005960",
    "scrollbarSlider.activeBackground": "#9E005980"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#9E0059",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#FF0054"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ff4c87"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ff3376"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#FFBD00"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ffc00c"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ff3f7e"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#FFBD00"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF0054"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ff266d"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#9E0059"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#FF0054"
      }
    }
  ]
}