  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#1e2a24",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#3a5844"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#88cc99"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#ffaa44"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#00ff88"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#00aaff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#cc88ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ccff00"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#00ffcc"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff00aa"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#88ff44"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#ffaa00"
      }
    },
    {
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#00ddbb"
      }
    },
    {
      "scope": "constant.other",
      "settings": {
        "foreground": "#77bbff"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ccffdd",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#77ddaa",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#00ccaa"
      }
    },
    {
      "scope": [
        "markup.list.numbered",
        "markup.list.unnumbered",
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#5a9866"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#4a7855",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.fenced_code",
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#88cc99"
      }
    },
    {
      "scope": "fenced_code.block.language",
      "settings": {
        "foreground": "#44aa77"
      }
    },
    {
      "scope": [
        "meta.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#00ddbb"
      }
    },
    {
      "scope": "punctuation.definition.heading",
      "settings": {
        "foreground": "#3a5844"
      }
    },
    {
      "scope": [
        "heading.1.markdown"
      ],
      "settings": {
        "foreground": "#6a9876",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "heading.2.markdown"
      ],
      "settings": {
        "foreground": "#5a8866",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "heading.3.markdown"
      ],
      "settings": {
        "foreground": "#5a8866",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "heading.4.markdown"
      ],
      "settings": {
        "foreground": "#4a7855",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "heading.5.markdown"
      ],
      "settings": {
        "foreground": "#4a7855",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "heading.6.markdown"
      ],
      "settings": {
        "foreground": "#3a6844",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.heading.markdown",
        "markup.heading",
        "entity.name.section.markdown"
      ],
      "settings": {
        "foreground": "#6a9876",
        "fontStyle": "bold"
      }
    }
  ]
}
