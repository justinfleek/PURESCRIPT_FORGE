  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#b0a8a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d71b1f"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#e35f62"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#df484b"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#e15457"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#d71b1f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#dd3d40"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#b0a8a6"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#d71b1f"
      }
    }
  ]
}