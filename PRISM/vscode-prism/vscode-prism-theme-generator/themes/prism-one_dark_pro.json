  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#e06c75",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#b6d5a1"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#accf93"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c87ede"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#b1d29a"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#c678dd"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#a7cc8d"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#98c379"
      }
    }
  ]
}