  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#A6F2B8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#74D2A9"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#62b28f"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#68bd98"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#1B445F"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#183d55"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6ec7a0"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#1B445F"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#74D2A9"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#68bd98"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#A6F2B8"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#74D2A9"
      }
    }
  ]
}