{
  "name": "straylight-prism",
  "displayName": "Straylight Prism Generator",
  "description": "Generate beautiful monochromatic themes from a single hue using OKLCH color science. Includes 38 curated presets with WCAG-verified contrast.",
  "version": "1.0.0",
  "publisher": "straylight-software",
  "author": {
    "name": "Straylight Software",
    "url": "https://straylight.software"
  },
  "contributors": [
    {
      "name": "@b7r6",
      "url": "https://github.com/b7r6"
    },
    {
      "name": "@j-pyxal",
      "url": "https://github.com/j-pyxal"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/straylight-software/prism"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^5.3.0"
  },
  "categories": ["Themes", "Other"],
  "keywords": [
    "theme", "color theme", "generator", "monochromatic", "211",
    "WCAG", "OKLCH", "accessible", "dark theme", "light theme",
    "straylight", "prism"
  ],
  "galleryBanner": {
    "color": "#0a0a0a",
    "theme": "dark"
  },
  "icon": "media/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "prismTheme.open211Generator",
        "title": "Straylight: Open Prism 211Â° Generator",
        "icon": "$(symbol-color)"
      },
      {
        "command": "prismTheme.openGallery",
        "title": "Straylight: Browse Theme Gallery"
      }
    ],
    "menus": {
      "commandPalette": [
        { "command": "prismTheme.open211Generator", "group": "straylight" }
      ]
    },
    "keybindings": [
      {
        "command": "prismTheme.open211Generator",
        "key": "ctrl+alt+2",
        "mac": "cmd+alt+2"
      }
    ],
    "themes": [
      { "label": "Tuned", "uiTheme": "vs-dark", "path": "./themes/prism-tuned.json" },
      { "label": "Memphis", "uiTheme": "vs-dark", "path": "./themes/prism-memphis.json" },
