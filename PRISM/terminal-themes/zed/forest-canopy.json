{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism forest-canopy",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism forest-canopy",
      "appearance": "dark",
      "style": {
        "background": "#0a100a",
        "editor.background": "#0a100a",
        "editor.foreground": "#d0e8d0",
        "editor.gutter.background": "#0a100a",
        "editor.line_highlight.background": "#111711",
        "editor.active_line_number": "#90d870",
        "editor.wrap_guide": "#508050",
        "terminal.background": "#0a100a",
        "terminal.foreground": "#d0e8d0",
        "terminal.ansi.black": "#0a100a",
        "terminal.ansi.red": "#f87171",
        "terminal.ansi.green": "#90d870",
        "terminal.ansi.yellow": "#fbbf24",
        "terminal.ansi.blue": "#60a5fa",
        "terminal.ansi.magenta": "#a78bfa",
        "terminal.ansi.cyan": "#22d3ee",
        "terminal.ansi.white": "#d0e8d0",
        "syntax": {
          "comment": {
            "color": "#406040",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#a78bfa",
            "font_weight": 600
          },
          "string": {
            "color": "#90d870"
          },
          "function": {
            "color": "#60a5fa"
          },
          "number": {
            "color": "#fb923c"
          },
          "type": {
            "color": "#90d870"
          },
          "variable": {
            "color": "#d0e8d0"
          },
          "constant": {
            "color": "#fb923c"
          },
          "attribute": {
            "color": "#90d870"
          },
          "property": {
            "color": "#d0e8d0"
          },
          "punctuation": {
            "color": "#d0e8d0"
          },
          "operator": {
            "color": "#a78bfa"
          }
        }
      }
    }
  ]
}