        "editor.background": "#0c0908",
        "editor.foreground": "#f8f0e0",
        "editor.gutter.background": "#0c0908",
        "editor.line_highlight.background": "#13100f",
        "editor.active_line_number": "#ffd93d",
        "editor.wrap_guide": "#5a4a40",
        "terminal.background": "#0c0908",
        "terminal.foreground": "#f8f0e0",
        "terminal.ansi.black": "#0c0908",
        "terminal.ansi.red": "#ff6b6b",
        "terminal.ansi.green": "#6bcb77",
        "terminal.ansi.yellow": "#ffd93d",
        "terminal.ansi.blue": "#4d96ff",
        "terminal.ansi.magenta": "#c792ea",
        "terminal.ansi.cyan": "#4d96ff",
        "terminal.ansi.white": "#f8f0e0",
