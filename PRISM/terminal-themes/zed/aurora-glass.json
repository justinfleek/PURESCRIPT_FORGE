{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism aurora-glass",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism aurora-glass",
      "appearance": "dark",
      "style": {
        "background": "#0a0815",
        "editor.background": "#0a0815",
        "editor.foreground": "#e8f0f8",
        "editor.gutter.background": "#0a0815",
        "editor.line_highlight.background": "#110f1c",
        "editor.active_line_number": "#a78bfa",
        "editor.wrap_guide": "#5a5570",
        "terminal.background": "#0a0815",
        "terminal.foreground": "#e8f0f8",
        "terminal.ansi.black": "#0a0815",
        "terminal.ansi.red": "#f87171",
        "terminal.ansi.green": "#4ade80",
        "terminal.ansi.yellow": "#a78bfa",
        "terminal.ansi.blue": "#60a5fa",
        "terminal.ansi.magenta": "#c084fc",
        "terminal.ansi.cyan": "#22d3ee",
        "terminal.ansi.white": "#e8f0f8",
        "syntax": {
          "comment": {
            "color": "#3a3550",
            "font_style": "italic"
