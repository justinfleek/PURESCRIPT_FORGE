{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism champagne-noir",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism champagne-noir",
      "appearance": "dark",
      "style": {
        "background": "#0a0a08",
        "editor.background": "#0a0a08",
        "editor.foreground": "#f0ece0",
        "editor.gutter.background": "#0a0a08",
        "editor.line_highlight.background": "#11110f",
        "editor.active_line_number": "#f7e7ce",
        "editor.wrap_guide": "#5a5a55",
        "terminal.background": "#0a0a08",
        "terminal.foreground": "#f0ece0",
        "terminal.ansi.black": "#0a0a08",
        "terminal.ansi.red": "#e86060",
        "terminal.ansi.green": "#60c080",
        "terminal.ansi.yellow": "#f7e7ce",
        "terminal.ansi.blue": "#6090e0",
        "terminal.ansi.magenta": "#a060e0",
        "terminal.ansi.cyan": "#60b0c0",
        "terminal.ansi.white": "#f0ece0",
        "syntax": {
          "comment": {
            "color": "#3a3a35",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#a060e0",
            "font_weight": 600
          },
          "string": {
            "color": "#60c080"
          },
          "function": {
            "color": "#6090e0"
          },
          "number": {
            "color": "#e89060"
          },
          "type": {
            "color": "#f7e7ce"
          },
          "variable": {
            "color": "#f0ece0"
          },
          "constant": {
            "color": "#e89060"
          },
          "attribute": {
            "color": "#f7e7ce"
          },
          "property": {
            "color": "#f0ece0"
          },
          "punctuation": {
            "color": "#f0ece0"
          },
          "operator": {
            "color": "#a060e0"
          }
        }
      }
    }
  ]
}