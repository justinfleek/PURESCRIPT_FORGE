{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism porcelain-moon",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism porcelain-moon",
      "appearance": "light",
      "style": {
        "background": "#e8eaee",
        "editor.background": "#e8eaee",
        "editor.foreground": "#1a1c20",
        "editor.gutter.background": "#e8eaee",
        "editor.line_highlight.background": "#e7e9ed",
        "editor.active_line_number": "#ca8a04",
        "editor.wrap_guide": "#707478",
        "terminal.background": "#e8eaee",
        "terminal.foreground": "#1a1c20",
        "terminal.ansi.black": "#1a1c20",
        "terminal.ansi.red": "#dc2626",
        "terminal.ansi.green": "#16a34a",
        "terminal.ansi.yellow": "#ca8a04",
        "terminal.ansi.blue": "#2563eb",
        "terminal.ansi.magenta": "#7c3aed",
        "terminal.ansi.cyan": "#0891b2",
        "terminal.ansi.white": "#e8eaee",
