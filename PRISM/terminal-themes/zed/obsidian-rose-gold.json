{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism obsidian-rose-gold",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism obsidian-rose-gold",
      "appearance": "dark",
      "style": {
        "background": "#050404",
        "editor.background": "#050404",
        "editor.foreground": "#f0e8e8",
        "editor.gutter.background": "#050404",
        "editor.line_highlight.background": "#0c0b0b",
        "editor.active_line_number": "#e8b4b8",
        "editor.wrap_guide": "#5a5555",
        "terminal.background": "#050404",
        "terminal.foreground": "#f0e8e8",
        "terminal.ansi.black": "#050404",
        "terminal.ansi.red": "#e87878",
        "terminal.ansi.green": "#78c8a8",
        "terminal.ansi.yellow": "#e8b4b8",
        "terminal.ansi.blue": "#7898e8",
        "terminal.ansi.magenta": "#c878e8",
        "terminal.ansi.cyan": "#78b8c8",
        "terminal.ansi.white": "#f0e8e8",
        "syntax": {
          "comment": {
            "color": "#3a3535",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#c878e8",
            "font_weight": 600
          },
          "string": {
            "color": "#78c8a8"
          },
          "function": {
            "color": "#7898e8"
          },
          "number": {
            "color": "#e8a878"
          },
          "type": {
            "color": "#e8b4b8"
          },
          "variable": {
            "color": "#f0e8e8"
          },
          "constant": {
            "color": "#e8a878"
          },
          "attribute": {
            "color": "#e8b4b8"
          },
          "property": {
            "color": "#f0e8e8"
          },
          "punctuation": {
            "color": "#f0e8e8"
          },
          "operator": {
            "color": "#c878e8"
          }
        }
      }
    }
  ]
}