{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism vesper",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism vesper",
      "appearance": "dark",
      "style": {
        "background": "#101010",
        "editor.background": "#101010",
        "editor.foreground": "#b0b0b0",
        "editor.gutter.background": "#101010",
        "editor.line_highlight.background": "#171717",
        "editor.active_line_number": "#ffc799",
        "editor.wrap_guide": "#606060",
        "terminal.background": "#101010",
        "terminal.foreground": "#b0b0b0",
        "terminal.ansi.black": "#101010",
        "terminal.ansi.red": "#f5a191",
        "terminal.ansi.green": "#99c794",
        "terminal.ansi.yellow": "#ffc799",
        "terminal.ansi.blue": "#99c794",
        "terminal.ansi.magenta": "#ffc799",
        "terminal.ansi.cyan": "#99c794",
        "terminal.ansi.white": "#b0b0b0",
        "syntax": {
          "comment": {
            "color": "#505050",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ffc799",
            "font_weight": 600
          },
          "string": {
            "color": "#99c794"
          },
          "function": {
            "color": "#ffc799"
          },
          "number": {
            "color": "#ffc799"
          },
          "type": {
            "color": "#ffc799"
          },
          "variable": {
            "color": "#b0b0b0"
          },
          "constant": {
            "color": "#ffc799"
          },
          "attribute": {
            "color": "#ffc799"
          },
          "property": {
            "color": "#b0b0b0"
          },
          "punctuation": {
            "color": "#b0b0b0"
          },
          "operator": {
            "color": "#ffc799"
          }
        }
      }
    }
  ]
}