{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism cobalt2",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism cobalt2",
      "appearance": "dark",
      "style": {
        "background": "#122738",
        "editor.background": "#122738",
        "editor.foreground": "#ffffff",
        "editor.gutter.background": "#122738",
        "editor.line_highlight.background": "#192d3d",
        "editor.active_line_number": "#ffc600",
        "editor.wrap_guide": "#6699cc",
        "terminal.background": "#122738",
        "terminal.foreground": "#ffffff",
        "terminal.ansi.black": "#122738",
        "terminal.ansi.red": "#ff628c",
        "terminal.ansi.green": "#3ad900",
        "terminal.ansi.yellow": "#ffc600",
        "terminal.ansi.blue": "#0088ff",
        "terminal.ansi.magenta": "#ff9d00",
        "terminal.ansi.cyan": "#80fcff",
        "terminal.ansi.white": "#ffffff",
        "syntax": {
          "comment": {
            "color": "#0088ff",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ff9d00",
            "font_weight": 600
          },
          "string": {
            "color": "#3ad900"
          },
          "function": {
            "color": "#ffc600"
          },
          "number": {
            "color": "#ff628c"
          },
          "type": {
            "color": "#80fcff"
          },
          "variable": {
            "color": "#ffffff"
          },
          "constant": {
            "color": "#ff628c"
          },
          "attribute": {
            "color": "#80fcff"
          },
          "property": {
            "color": "#ffffff"
          },
          "punctuation": {
            "color": "#ffffff"
          },
          "operator": {
            "color": "#ff9d00"
          }
        }
      }
    }
  ]
}