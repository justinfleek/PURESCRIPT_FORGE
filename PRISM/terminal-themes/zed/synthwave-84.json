{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism synthwave-84",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism synthwave-84",
      "appearance": "dark",
      "style": {
        "background": "#1a1024",
        "editor.background": "#1a1024",
        "editor.foreground": "#f0e0ff",
        "editor.gutter.background": "#1a1024",
        "editor.line_highlight.background": "#20172a",
        "editor.active_line_number": "#ff7edb",
        "editor.wrap_guide": "#8060a0",
        "terminal.background": "#1a1024",
        "terminal.foreground": "#f0e0ff",
        "terminal.ansi.black": "#1a1024",
        "terminal.ansi.red": "#fe4450",
        "terminal.ansi.green": "#72f1b8",
        "terminal.ansi.yellow": "#fede5d",
        "terminal.ansi.blue": "#36f9f6",
        "terminal.ansi.magenta": "#ff7edb",
        "terminal.ansi.cyan": "#36f9f6",
        "terminal.ansi.white": "#f0e0ff",
        "syntax": {
          "comment": {
            "color": "#8060a0",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#fede5d",
            "font_weight": 600
          },
          "string": {
            "color": "#72f1b8"
          },
          "function": {
            "color": "#36f9f6"
          },
          "number": {
            "color": "#f97e72"
          },
          "type": {
            "color": "#ff7edb"
          },
          "variable": {
            "color": "#f0e0ff"
          },
          "constant": {
            "color": "#f97e72"
          },
          "attribute": {
            "color": "#ff7edb"
          },
          "property": {
            "color": "#f0e0ff"
          },
          "punctuation": {
            "color": "#f0e0ff"
          },
          "operator": {
            "color": "#fede5d"
          }
        }
      }
    }
  ]
}