{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism blood-moon",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism blood-moon",
      "appearance": "dark",
      "style": {
        "background": "#080404",
        "editor.background": "#080404",
        "editor.foreground": "#f0d0d0",
        "editor.gutter.background": "#080404",
        "editor.line_highlight.background": "#0f0b0b",
        "editor.active_line_number": "#ff1744",
        "editor.wrap_guide": "#884040",
        "terminal.background": "#080404",
        "terminal.foreground": "#f0d0d0",
        "terminal.ansi.black": "#080404",
        "terminal.ansi.red": "#ff1744",
        "terminal.ansi.green": "#4ade80",
        "terminal.ansi.yellow": "#ff6b35",
        "terminal.ansi.blue": "#60a5fa",
        "terminal.ansi.magenta": "#c01030",
        "terminal.ansi.cyan": "#22d3ee",
        "terminal.ansi.white": "#f0d0d0",
        "syntax": {
          "comment": {
            "color": "#704040",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ff1744",
            "font_weight": 600
          },
          "string": {
            "color": "#4ade80"
          },
          "function": {
            "color": "#ff6b35"
          },
          "number": {
            "color": "#ff6b35"
          },
          "type": {
            "color": "#ff1744"
          },
          "variable": {
            "color": "#f0d0d0"
          },
          "constant": {
            "color": "#ff6b35"
          },
          "attribute": {
            "color": "#ff1744"
          },
          "property": {
            "color": "#f0d0d0"
          },
          "punctuation": {
            "color": "#f0d0d0"
          },
          "operator": {
            "color": "#ff1744"
          }
        }
      }
    }
  ]
}