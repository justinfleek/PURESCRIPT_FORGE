{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism fleek-light",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism fleek-light",
      "appearance": "light",
      "style": {
        "background": "#fafaf8",
        "editor.background": "#fafaf8",
        "editor.foreground": "#1a1a18",
        "editor.gutter.background": "#fafaf8",
        "editor.line_highlight.background": "#f9f9f7",
        "editor.active_line_number": "#2a9860",
        "editor.wrap_guide": "#6a6a68",
        "terminal.background": "#fafaf8",
        "terminal.foreground": "#1a1a18",
        "terminal.ansi.black": "#1a1a18",
        "terminal.ansi.red": "#d85030",
        "terminal.ansi.green": "#2a9860",
        "terminal.ansi.yellow": "#b08010",
        "terminal.ansi.blue": "#0088c8",
        "terminal.ansi.magenta": "#8a7858",
        "terminal.ansi.cyan": "#0088c8",
        "terminal.ansi.white": "#fafaf8",
        "syntax": {
          "comment": {
            "color": "#9a9a98",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#d85030",
            "font_weight": 600
          },
          "string": {
            "color": "#2a9860"
          },
          "function": {
            "color": "#0088c8"
          },
          "number": {
            "color": "#b08010"
          },
          "type": {
            "color": "#2a9860"
          },
          "variable": {
            "color": "#1a1a18"
          },
          "constant": {
            "color": "#b08010"
          },
          "attribute": {
            "color": "#2a9860"
          },
          "property": {
            "color": "#1a1a18"
          },
          "punctuation": {
            "color": "#1a1a18"
          },
          "operator": {
            "color": "#d85030"
          }
        }
      }
    }
  ]
}