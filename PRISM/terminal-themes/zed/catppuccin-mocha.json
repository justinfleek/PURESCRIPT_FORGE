{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism catppuccin-mocha",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism catppuccin-mocha",
      "appearance": "dark",
      "style": {
        "background": "#1e1e2e",
        "editor.background": "#1e1e2e",
        "editor.foreground": "#cdd6f4",
        "editor.gutter.background": "#1e1e2e",
        "editor.line_highlight.background": "#242434",
        "editor.active_line_number": "#cba6f7",
        "editor.wrap_guide": "#6c7086",
        "terminal.background": "#1e1e2e",
        "terminal.foreground": "#cdd6f4",
        "terminal.ansi.black": "#1e1e2e",
        "terminal.ansi.red": "#f38ba8",
        "terminal.ansi.green": "#a6e3a1",
        "terminal.ansi.yellow": "#f9e2af",
        "terminal.ansi.blue": "#89b4fa",
        "terminal.ansi.magenta": "#cba6f7",
        "terminal.ansi.cyan": "#89dceb",
        "terminal.ansi.white": "#cdd6f4",
        "syntax": {
          "comment": {
            "color": "#6c7086",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#cba6f7",
            "font_weight": 600
          },
          "string": {
            "color": "#a6e3a1"
          },
          "function": {
            "color": "#89b4fa"
          },
          "number": {
            "color": "#fab387"
          },
          "type": {
            "color": "#f9e2af"
          },
          "variable": {
            "color": "#cdd6f4"
          },
          "constant": {
            "color": "#fab387"
          },
          "attribute": {
            "color": "#f9e2af"
          },
          "property": {
            "color": "#cdd6f4"
          },
          "punctuation": {
            "color": "#cdd6f4"
          },
          "operator": {
            "color": "#cba6f7"
          }
        }
