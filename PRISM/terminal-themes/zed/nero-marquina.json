{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism nero-marquina",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism nero-marquina",
      "appearance": "dark",
      "style": {
        "background": "#08080a",
        "editor.background": "#08080a",
        "editor.foreground": "#e8e4dc",
        "editor.gutter.background": "#08080a",
        "editor.line_highlight.background": "#0f0f11",
        "editor.active_line_number": "#d4af37",
        "editor.wrap_guide": "#5a5a68",
        "terminal.background": "#08080a",
        "terminal.foreground": "#e8e4dc",
        "terminal.ansi.black": "#08080a",
        "terminal.ansi.red": "#e85454",
        "terminal.ansi.green": "#54c878",
        "terminal.ansi.yellow": "#d4af37",
        "terminal.ansi.blue": "#5494e8",
        "terminal.ansi.magenta": "#b454e8",
        "terminal.ansi.cyan": "#54b4b8",
        "terminal.ansi.white": "#e8e4dc",
        "syntax": {
          "comment": {
            "color": "#3a3a45",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#b454e8",
            "font_weight": 600
          },
          "string": {
            "color": "#54c878"
          },
          "function": {
            "color": "#5494e8"
          },
          "number": {
            "color": "#e8a054"
          },
          "type": {
            "color": "#d4af37"
          },
          "variable": {
            "color": "#e8e4dc"
          },
          "constant": {
            "color": "#e8a054"
          },
          "attribute": {
            "color": "#d4af37"
          },
          "property": {
            "color": "#e8e4dc"
          },
          "punctuation": {
            "color": "#e8e4dc"
          },
          "operator": {
            "color": "#b454e8"
          }
        }
      }
    }
  ]
}