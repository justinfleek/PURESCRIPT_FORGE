{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Prism holographic",
  "author": "Prism Theme System",
  "themes": [
    {
      "name": "Prism holographic",
      "appearance": "dark",
      "style": {
        "background": "#0a0810",
        "editor.background": "#0a0810",
        "editor.foreground": "#e8e0f8",
        "editor.gutter.background": "#0a0810",
        "editor.line_highlight.background": "#110f17",
        "editor.active_line_number": "#00ffff",
        "editor.wrap_guide": "#6060a0",
        "terminal.background": "#0a0810",
        "terminal.foreground": "#e8e0f8",
        "terminal.ansi.black": "#0a0810",
        "terminal.ansi.red": "#ff0080",
        "terminal.ansi.green": "#00ff80",
        "terminal.ansi.yellow": "#ffcc00",
        "terminal.ansi.blue": "#0080ff",
        "terminal.ansi.magenta": "#8000ff",
        "terminal.ansi.cyan": "#00ffff",
        "terminal.ansi.white": "#e8e0f8",
        "syntax": {
          "comment": {
            "color": "#505080",
            "font_style": "italic"
          },
          "keyword": {
            "color": "#ff0080",
            "font_weight": 600
          },
          "string": {
            "color": "#00ff80"
          },
          "function": {
            "color": "#0080ff"
          },
          "number": {
            "color": "#ffcc00"
          },
          "type": {
            "color": "#00ffff"
          },
          "variable": {
            "color": "#e8e0f8"
          },
          "constant": {
            "color": "#ffcc00"
          },
          "attribute": {
            "color": "#00ffff"
          },
          "property": {
            "color": "#e8e0f8"
          },
          "punctuation": {
            "color": "#e8e0f8"
          },
          "operator": {
            "color": "#ff0080"
          }
        }
      }
    }
  ]
}