{
  "name": "Prism ocean-depths",
  "variables": {
    "bg": "#0a1018",
    "fg": "#d0e0f0"
  },
  "globals": {
    "background": "#0a1018",
    "foreground": "#d0e0f0",
    "caret": "#61e8e1",
    "selection": "#44475a",
    "line_highlight": "#11171e"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#406080",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a78bfa"
    },
    {
      "scope": "string",
      "foreground": "#61e8e1"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fb923c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#61e8e1"
    },
    {
      "scope": "variable",
      "foreground": "#d0e0f0"
    }
  ]
}