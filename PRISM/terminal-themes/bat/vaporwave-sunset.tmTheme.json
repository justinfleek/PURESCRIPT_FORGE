{
  "name": "Prism vaporwave-sunset",
  "variables": {
    "bg": "#100818",
    "fg": "#f0e0ff"
  },
  "globals": {
    "background": "#100818",
    "foreground": "#f0e0ff",
    "caret": "#00ffff",
    "selection": "#44475a",
    "line_highlight": "#170f1e"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#705090",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ff6b9d"
    },
    {
      "scope": "string",
      "foreground": "#00ffaa"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#00ffff"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ffcc00"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#c44dff"
    },
    {
      "scope": "variable",
      "foreground": "#f0e0ff"
    }
  ]
}