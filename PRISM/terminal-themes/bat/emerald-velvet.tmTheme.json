{
  "name": "Prism emerald-velvet",
  "variables": {
    "bg": "#040806",
    "fg": "#e0f0e0"
  },
  "globals": {
    "background": "#040806",
    "foreground": "#e0f0e0",
    "caret": "#d4af37",
    "selection": "#44475a",
    "line_highlight": "#0b0f0d"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#283828",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a78bfa"
    },
    {
      "scope": "string",
      "foreground": "#50c878"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#d4af37"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#d4af37"
    },
    {
      "scope": "variable",
      "foreground": "#e0f0e0"
    }
  ]
}