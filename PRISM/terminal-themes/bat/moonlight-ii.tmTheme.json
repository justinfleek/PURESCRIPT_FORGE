{
  "name": "Prism moonlight-ii",
  "variables": {
    "bg": "#1b1d2c",
    "fg": "#c8d3f5"
  },
  "globals": {
    "background": "#1b1d2c",
    "foreground": "#c8d3f5",
    "caret": "#82aaff",
    "selection": "#44475a",
    "line_highlight": "#212332"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#636da6",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#c099ff"
    },
    {
      "scope": "string",
      "foreground": "#c3e88d"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#82aaff"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ff966c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#ffc777"
    },
    {
      "scope": "variable",
      "foreground": "#c8d3f5"
    }
  ]
}