{
  "name": "Prism soft-charcoal",
  "variables": {
    "bg": "#2a2a32",
    "fg": "#e8e8f0"
  },
  "globals": {
    "background": "#2a2a32",
    "foreground": "#e8e8f0",
    "caret": "#fbbf24",
    "selection": "#44475a",
    "line_highlight": "#303038"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#50505a",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a78bfa"
    },
    {
      "scope": "string",
      "foreground": "#4ade80"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fb923c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#fbbf24"
    },
    {
      "scope": "variable",
      "foreground": "#e8e8f0"
    }
  ]
}