{
  "name": "Prism tide-pool",
  "variables": {
    "bg": "#06090c",
    "fg": "#e0f0f8"
  },
  "globals": {
    "background": "#06090c",
    "foreground": "#e0f0f8",
    "caret": "#40e0d0",
    "selection": "#44475a",
    "line_highlight": "#0d1013"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#283540",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a78bfa"
    },
    {
      "scope": "string",
      "foreground": "#00ff88"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#40e0d0"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fb923c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#40e0d0"
    },
    {
      "scope": "variable",
      "foreground": "#e0f0f8"
    }
  ]
}