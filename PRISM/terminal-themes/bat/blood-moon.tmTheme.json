{
  "name": "Prism blood-moon",
  "variables": {
    "bg": "#080404",
    "fg": "#f0d0d0"
  },
  "globals": {
    "background": "#080404",
    "foreground": "#f0d0d0",
    "caret": "#ff1744",
    "selection": "#44475a",
    "line_highlight": "#0f0b0b"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#704040",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ff1744"
    },
    {
      "scope": "string",
      "foreground": "#4ade80"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#ff6b35"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ff6b35"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#ff1744"
    },
    {
      "scope": "variable",
      "foreground": "#f0d0d0"
    }
  ]
}