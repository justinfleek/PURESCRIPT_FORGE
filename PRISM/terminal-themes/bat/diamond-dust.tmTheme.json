{
  "name": "Prism diamond-dust",
  "variables": {
    "bg": "#08090d",
    "fg": "#e8f0ff"
  },
  "globals": {
    "background": "#08090d",
    "foreground": "#e8f0ff",
    "caret": "#38bdf8",
    "selection": "#44475a",
    "line_highlight": "#0f1014"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#303540",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#818cf8"
    },
    {
      "scope": "string",
      "foreground": "#4ade80"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fb923c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#38bdf8"
    },
    {
      "scope": "variable",
      "foreground": "#e8f0ff"
    }
  ]
}