{
  "name": "Prism midnight-sapphire",
  "variables": {
    "bg": "#050810",
    "fg": "#e0e8f8"
  },
  "globals": {
    "background": "#050810",
    "foreground": "#e0e8f8",
    "caret": "#c0c0d0",
    "selection": "#44475a",
    "line_highlight": "#0c0f17"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#2a3548",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a78bfa"
    },
    {
      "scope": "string",
      "foreground": "#6ee7b7"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#60a5fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fb923c"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#c0c0d0"
    },
    {
      "scope": "variable",
      "foreground": "#e0e8f8"
    }
  ]
}