{
  "name": "Prism zen-garden",
  "variables": {
    "bg": "#101210",
    "fg": "#e8f0e8"
  },
  "globals": {
    "background": "#101210",
    "foreground": "#e8f0e8",
    "caret": "#c8b878",
    "selection": "#44475a",
    "line_highlight": "#171917"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#404540",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#a078a0"
    },
    {
      "scope": "string",
      "foreground": "#78b878"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#7090b8"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#d4a870"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#c8b878"
    },
    {
      "scope": "variable",
      "foreground": "#e8f0e8"
    }
  ]
}