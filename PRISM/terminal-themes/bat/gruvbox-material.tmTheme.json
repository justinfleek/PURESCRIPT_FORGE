{
  "name": "Prism gruvbox-material",
  "variables": {
    "bg": "#1d2021",
    "fg": "#d4be98"
  },
  "globals": {
    "background": "#1d2021",
    "foreground": "#d4be98",
    "caret": "#d8a657",
    "selection": "#44475a",
    "line_highlight": "#232627"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#7c6f64",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#ea6962"
    },
    {
      "scope": "string",
      "foreground": "#a9b665"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#a9b665"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#d3869b"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#d8a657"
    },
    {
      "scope": "variable",
      "foreground": "#d4be98"
    }
  ]
}