{
  "name": "Prism tokyo-night-bento",
  "variables": {
    "bg": "#0f0f18",
    "fg": "#e8e8f0"
  },
  "globals": {
    "background": "#0f0f18",
    "foreground": "#e8e8f0",
    "caret": "#bb9af7",
    "selection": "#44475a",
    "line_highlight": "#16161e"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#3a3a4a",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#bb9af7"
    },
    {
      "scope": "string",
      "foreground": "#9ece6a"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#7aa2f7"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#ff9e64"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#e0af68"
    },
    {
      "scope": "variable",
      "foreground": "#e8e8f0"
    }
  ]
}