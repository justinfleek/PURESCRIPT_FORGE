{
  "name": "Prism catppuccin-mocha",
  "variables": {
    "bg": "#1e1e2e",
    "fg": "#cdd6f4"
  },
  "globals": {
    "background": "#1e1e2e",
    "foreground": "#cdd6f4",
    "caret": "#cba6f7",
    "selection": "#44475a",
    "line_highlight": "#242434"
  },
  "rules": [
    {
      "scope": "comment",
      "foreground": "#6c7086",
      "font_style": "italic"
    },
    {
      "scope": "keyword",
      "foreground": "#cba6f7"
    },
    {
      "scope": "string",
      "foreground": "#a6e3a1"
    },
    {
      "scope": "entity.name.function",
      "foreground": "#89b4fa"
    },
    {
      "scope": "constant.numeric",
      "foreground": "#fab387"
    },
    {
      "scope": "entity.name.type, support.type",
      "foreground": "#f9e2af"
    },
    {
      "scope": "variable",
      "foreground": "#cdd6f4"
    }
  ]
}