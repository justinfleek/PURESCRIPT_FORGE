    "scrollbarSlider.hoverBackground": "#ff79c660",
    "scrollbarSlider.activeBackground": "#ff79c680"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#ff79c6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#d0b3fa"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#caa8fa"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f1fa91"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#cdaefa"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#f1fa8c"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#c6a3f9"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#bd93f9"
      }
    }
  ]
}