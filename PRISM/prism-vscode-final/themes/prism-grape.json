  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#3C096C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7B2CBF"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#a26bd2"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#9556cb"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F0D6FF"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#f0d8ff"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#9c60cf"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#F0D6FF"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7B2CBF"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#8e4bc8"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#3C096C"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#7B2CBF"
      }
    }
  ]
}