  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6e6a86",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#31748f"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#6e9db0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#5a8fa5"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#c6abe8"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#6496ab"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#31748f"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4f889f"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#31748f"
      }
    }
  ]
}