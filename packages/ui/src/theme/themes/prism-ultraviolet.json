    "scrollbarSlider.hoverBackground": "#bd93f960",
    "scrollbarSlider.activeBackground": "#bd93f980"
  },
  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#bd93f9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#ffa1d7"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ff93d1"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#58fa81"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#ff9ad4"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#50fa7b"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#ff8dce"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#bd93f9"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#ff79c6"
      }
    }
  ]
}