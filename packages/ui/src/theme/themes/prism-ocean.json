  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#0E566C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#34A0A4"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#70bcbf"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#5cb3b6"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#348FD5"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#3e94d7"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#66b7ba"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#348FD5"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#34A0A4"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#52aeb1"
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#0E566C"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#34A0A4"
      }
    }
  ]
}