# Buck2 Configuration for CODER
#
# This is the main Buck2 configuration file.
# .buckconfig.local is auto-generated by aleph.build module with Nix store paths.

[repositories]
# Use the straylight-buck2-prelude (symlinked to nix/build/prelude by aleph.build)
prelude = nix/build/prelude

# Toolchains directory (generated by aleph.build)
toolchains = nix/build/toolchains

[build]
# Use Nix store paths from .buckconfig.local
# This is auto-generated by aleph.build module

[project]
# Ignore patterns
ignore = .git
ignore = .github
ignore = .cursor
ignore = node_modules
ignore = output
ignore = result
ignore = result-*
